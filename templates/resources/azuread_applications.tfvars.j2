azuread_applications = {
{% for key, app in resources[tfstate_resource].resources[subscription_key].azuread_applications.items() %}
  {{ key }} = {
    application_name = "{{ app.application_name }}"
{% if app.owners is defined %}
    owners = {{ app.owners| string | replace('None','[]') | replace('\'','\"')  }}
{% endif %}
  }
{% endfor %}
}